@using DataContext
@using DataContext.Constants
@using DataContext.Until
@if (Model == null)
{
    <p>@(ASS_35.Resources.HomeText.No_data)</p>
}
else
{
    var model = (List<m_filethongbao>)Model;
    <ul>
        @foreach (var item in model)
        {
            <li class="item-file-pdf" onclick="PreviewPdf(this)" data-toggle='modal' data-target='.bs-example-modal-lg' data-url="@item.linkpath" style="cursor: pointer;"><span style="color: blue;">@item.tenfile</span></li>
        }
    </ul>
}
