@using DataContext.DAL;


@{
    var model = (DataContext.m_lienhe)Model;
}

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/LayoutVi.cshtml";
}

<div class="body-contact">
    <div class="head-contact">
        <img src="images/banercontact.png">
        <div class="contact">
            Liên hệ
        </div>
    </div>

    <div class="contact-noidung" style="padding-bottom:200px">
        @if (model != null)
        {
            @Html.Raw(model.noidung)
        }
        else
        {
        <h3>Đang cập nhật</h3>
        }

    </div>
</div>
